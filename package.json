{
  "name": "express-api-server-starter",
  "version": "0.0.1",
  "description": "An extensible, authenticated REST server with event logging",
  "main": "index.js",
  "engines": {
    "node": "^11.8.0",
    "npm": ">= 6.10.1",
    "nvm": ">= 0.34.0"
  },
  "directories": {
    "doc": "docs"
  },
  "author": "CharmedSatyr <z@charmed.tech>",
  "license": "GPL-3.0-or-later",
  "keywords": [
    "express",
    "server",
    "template"
  ],
  "homepage": "https://github.com/CharmedSatyr/express-api-server-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CharmedSatyr/express-api-server-starter.git"
  },
  "bugs": {
    "url": "https://github.com/CharmedSatyr/express-api-server-starter/issues"
  },
  "scripts": {
    "start": "node index.js",
    "watch": "nodemon index.js",
    "lint": "eslint index.js && eslint 'server/**/*'",
    "test": "jest --coverage --verbose --detectOpenHandles",
    "test-watch": "jest --coverage --verbose --watch",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | codacy-coverage --token ade4867bae464dfda77c41c94f40cec2",
    "doc": "jsdoc -c ./config/jsdoc.config.json",
    "clean": "rm -rf docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test",
      "pre-push": "export CI=true && lint-staged && npm run coverage"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -c './tslint.json'"
    ]
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^5.1.5",
    "mongoose": "^5.6.4",
    "mongoose-schema-jsonschema": "^1.2.1",
    "morgan": "^1.9.1",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "codacy-coverage": "^3.4.0",
    "eslint": "^6.0.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.2",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
