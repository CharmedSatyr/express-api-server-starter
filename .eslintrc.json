{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jest": true,
    "es6": true
  },
  "globals": {
    "err": true,
    "req": true,
    "res": true,
    "next": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^req$|^res$|^next$"
      }
    ]
  }
}
